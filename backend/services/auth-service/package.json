{
  "name": "@talk-legally/auth-service",
  "version": "1.0.0",
  "description": "Authentication and Authorization Service",
  "main": "dist/services/auth-service/src/main.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "node dist/services/auth-service/src/main.js",
    "start:dev": "concurrently \"tsc -w\" \"tsc-alias -w\" \"nodemon --watch dist/services/auth-service/src --delay 2 dist/services/auth-service/src/main.js\"",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/services/auth-service/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage"
  },
  "dependencies": {
    "@nestjs-modules/ioredis": "^1.2.4",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^11.1.12",
    "@nestjs/typeorm": "^10.0.0",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.4.5",
    "ioredis": "^5.3.0",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.0",
    "redis": "^4.6.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/node": "^20.3.1",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/passport-jwt": "^3.0.9",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "concurrently": "^9.2.1",
    "eslint": "^8.42.0",
    "jest": "^29.5.0",
    "nodemon": "^3.1.11",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.1.3"
  }
}