{
  "name": "@talk-legally/session-service",
  "version": "1.0.0",
  "description": "Session Management Service",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "node dist/services/session-service/src/main.js",
    "start:dev": "concurrently \"tsc -w\" \"tsc-alias -w\" \"nodemon --watch dist/services/session-service/src --delay 2 dist/services/session-service/src/main.js\"",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/services/session-service/src/main.js"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/schedule": "^3.0.0",
    "@nestjs/typeorm": "^10.0.0",
    "agora-token": "^2.0.4",
    "axios": "^1.4.0",
    "class-validator": "^0.14.0",
    "dotenv": "^16.4.5",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@types/node": "^20.3.1",
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.11",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.1.3"
  }
}